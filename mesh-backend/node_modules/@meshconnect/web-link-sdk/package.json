{
  "name": "@meshconnect/web-link-sdk",
  "version": "3.1.12",
  "description": "A client-side JS library for integrating with Mesh Connect",
  "exports": "./index.js",
  "license": "MIT",
  "repository": "https://github.com/FrontFin/mesh-web-sdk.git",
  "homepage": "https://github.com/FrontFin/mesh-web-sdk",
  "devDependencies": {
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.20.0",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.5.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.2.1",
    "ts-jest": "^29.1.2",
    "util": "^0.12.4"
  },
  "dependencies": {
    "@meshconnect/node-api": "^2.0.20",
    "@meshconnect/solana-web3.js": "^1.0.4",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "ethers": "^6.11.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coverageDirectory": "./coverage",
    "transform": {
      "^.+\\.[tj]sx?$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
          ]
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@meshconnect/solana-web3.js|uuid|jayson|superstruct|@noble|@meshconnect|ethers)/)"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  },
  "module": "./index.js",
  "main": "./index.js",
  "types": "./index.d.ts"
}